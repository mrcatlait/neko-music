{
  "name": "@neko/permissions",
  "private": true,
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@neko/eslint-config": "*",
    "typescript": "~5.7.2"
  },
  "prettier": "@neko/eslint-config/prettier"
}
